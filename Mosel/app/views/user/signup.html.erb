      <div class="signup">
        <div class="row">
          <div class="col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3">
            <form class="form-horizontal signin-form">
              <div class="form-group">
                <label for="email" class="col-sm-4 control-label">Email</label>
                <div class="col-sm-8">
                  <input type="email" class="form-control" id="email" placeholder="Email">
                </div>
              </div>
              <div class="form-group">
                <label for="password" class="col-sm-4 control-label">Password</label>
                <div class="col-sm-8">
                  <input type="password" class="form-control" id="password" placeholder="Password">
                </div>
              </div>
              <div class="form-group">
                <label for="password_confirm" class="col-sm-4 control-label">Password Confirm</label>
                <div class="col-sm-8">
                  <input type="password" class="form-control" id="password_confirm" placeholder="Password Confirm">
                </div>
              </div>
              <div class="form-group">
                <div class="col-sm-offset-4 col-sm-8">
                  <button type="submit" class="btn btn-primary">Sign Up</button>
                </div>
              </div>
            </form>
            <div class="signin">
              Already have an account? Please <a href="/user/signin/">Sign In</a>
            </div>
          </div>
        </div>
      </div>
